<% content_for :header_left do %>
  <%= content_tag :h1, [
      link_to('SKU Prefixes', admin_sku_prefixes_path),
      'New SKU Prefix'
  ].join(' / ').html_safe %>
<% end %>

<%= render partial: 'admin/shared/header' %>

<%= content_tag :div, class: 'row' do %>
  <%= content_tag :div, class: 'col-md-12' do %>
    <%= form_for [:admin, @sku_prefix] do |f| %>
      <%= field_set_tag nil, 'data-hook': 'sku_prefix_new' do %>
        <%= render partial: 'form', locals: { f: f } %>
      <% end %>

      <%= form_buttons admin_sku_prefixes_path %>
    <% end %>
  <% end %>
<% end %>
